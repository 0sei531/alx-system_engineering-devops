[Unit]
Description=HBNB Gunicorn instance
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app --error-logfile /tmp/hbnb-error.log --access-logfile /tmp/hbnb-access.log
Restart=always
SyslogIdentifier=gunicorn

[Install]
WantedBy=multi-user.target
